<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomMenuItem">
        <attr name="menuIconSrc" format="reference" />
        <attr name="menuText" format="string" />
    </declare-styleable>
    <declare-styleable name="FlexRecyclerView">
        <attr name="flexDirectionRecycler">
            <enum name="row" value="0"/>
            <enum name="row_reverse" value="1"/>
            <enum name="column" value="2"/>
            <enum name="column_reverse" value="3"/>
        </attr>
        <attr name="flexWrapRecycler">
            <enum name="nowrap" value="0"/>
            <enum name="wrap" value="1"/>
            <enum name="wrap_reverse" value="2"/>
        </attr>
        <attr name="justifyContentRecycler">
            <enum name="flex_start" value="0"/>
            <enum name="flex_end" value="1"/>
            <enum name="center" value="2"/>
            <enum name="space_between" value="3"/>
            <enum name="space_around" value="4"/>
            <enum name="space_evenly" value="5"/>
        </attr>
    </declare-styleable>
</resources>